{
    "name": "yiisoft/security",
    "type": "library",
    "description": "Security utilities",
    "keywords": [
        "security",
        "random",
        "password",
        "hash",
        "encryption",
        "decryption",
        "signature",
        "token masking",
        "mac"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/security/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/security"
    },
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/strings": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.3",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Security\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Security\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
